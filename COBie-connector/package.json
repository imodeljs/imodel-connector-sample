{
  "name": "@bentley/cobie-connector",
  "private": true,
  "version": "1.0.12",
  "description": "iModel Connector to push Sample COBie Data to iModelHub",
  "scripts": {
    "build": "tsc 1>&2",
    "clean": "rimraf lib",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "blint": "npm run build && npm run lint",
    "pretest": "cpx ./src/test/logging.config.json ./lib/test && cpx \"./src/test/assets/**/*\" ./lib/test/assets && cpx \"./src/schema/*.ecschema.xml\" ./lib/schema",
    "test": "betools test --testDir=\"./lib/test/unit\"",
    "test:integration": "npm run pretest && betools test --testDir=\"./lib/test/integration\"",
    "start": "node ./node_modules/@bentley/imodeljs-backend/lib/iModelBridgeFwkMain.js"
  },
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "dependencies": {
    "@bentley/backend-itwin-client": "^2.12.0",
    "@bentley/bentleyjs-core": "^2.12.0",
    "@bentley/ecschema-metadata": "^2.12.0",
    "@bentley/frontend-authorization-client": "^2.12.0",
    "@bentley/geometry-core": "^2.12.0",
    "@bentley/imodel-bridge": "^2.12.0",
    "@bentley/imodelhub-client": "^2.12.0",
    "@bentley/imodeljs-backend": "^2.12.0",
    "@bentley/imodeljs-common": "^2.12.0",
    "@bentley/imodeljs-i18n": "^2.12.0",
    "@bentley/itwin-client": "^2.12.0",
    "@bentley/logger-config": "^2.12.0",
    "@bentley/rbac-client": "^2.12.0",
    "@bentley/telemetry-client": "^2.12.0",
    "bunyan": "^1.8.13",
    "bunyan-seq": "^0.2.0",
    "draco3d": "^1.3.6",
    "open": "^7.1.0",
    "request-promise": "^4.2.6",
    "sqlite": "^4.0.12",
    "sqlite3": "^5.0.0",
    "three": "^0.116.1",
    "username": "^5.1.0",
    "xmldom": "^0.3.0"
  },
  "devDependencies": {
    "@bentley/build-tools": "^2.12.0",
    "@bentley/eslint-plugin": "^2.12.0",
    "@bentley/oidc-signin-tool": "^2.12.0",
    "@types/chai": "^4.2.12",
    "@types/jquery": "^3.5.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.17.28",
    "@types/object-hash": "^1.3.3",
    "@types/sqlite3": "^3.1.6",
    "@types/xmldom": "^0.1.30",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^5.2.0",
    "typescript": "^3.9.7"
  },
  "eslintConfig": {
    "plugins": [
      "@bentley"
    ],
    "extends": "plugin:@bentley/imodeljs-recommended"
  }
}
